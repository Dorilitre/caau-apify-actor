{
  "name": "tiktok-shop-scraper",
  "version": "1.0.0",
  "description": "TikTok Shop BR Actor - Scrapes TikTok Shop products focused on Brazil with Supabase schema mapping",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write src/**/*.{ts,js,json}",
    "type-check": "tsc --noEmit",
    "apify:push": "apify push"
  },
  "keywords": [
    "apify",
    "actor",
    "tiktok",
    "shop",
    "brazil",
    "scraping",
    "supabase"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "apify": "^3.1.10",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.10.0"
}
